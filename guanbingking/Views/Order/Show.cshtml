                        </label>
                </div>

                <div class="layui-form-item">
                    <label for="L_pass" class="layui-form-label">
                        商品种类：</label>
                    <label class="layui-form-label l2">
                    @Model.productClass
                        </label>
                </div>

                <div class="layui-form-item">
                    <label for="L_pass" class="layui-form-label">
